<h1>Your Facebook Information</h1>
<%= link_to 'Logout', dashboard_index_path, class: 'logout' %>
<img src='https://graph.facebook.com/<%= @profile["id"] %>/picture' alt='picture'>
<div class="profile_info"> 
   <h4>Name: <%= @profile["name"] %></h2>
   <h4>Email Address: <%= @profile["email"] %></h4>
</div>
<h3>Facebook Friends Who Also Use This App</h3>
<% @friends.each do |friend| %>
   <div class="contact_box">
   	<h4><%= friend["name"] %></h4>
      <%= image_tag friend["picture"]["data"]["url"] %>
   </div>
<% end %> 